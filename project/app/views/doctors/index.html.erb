<p id="notice"><%= notice %></p>

<h1>Doctors</h1>

<table>
  <thead>
    <tr>
      <th>Docname</th>
      <th>Specialization</th>
      <th>Department</th>
      <th>Fee</th>
      <th>Schedule</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @doctors.each do |doctor| %>
      <tr>
        <td><%= doctor.docName %></td>
        <td><%= doctor.specialization %></td>
        <td><%= doctor.department %></td>
        <td><%= doctor.fee %></td>
        <td><%= doctor.schedule %></td>
        <td><%= link_to 'Show', doctor %></td>
        <td><%= link_to 'Edit', edit_doctor_path(doctor) %></td>
        <td><%= link_to 'Destroy', doctor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Doctor', new_doctor_path %>
<%= link_to 'Back', dashboard_index_path %>