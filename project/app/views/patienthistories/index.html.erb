<p id="notice"><%= notice %></p>

<h1>Patient Histories</h1>

<table>
  <thead>
    <tr>
      <th>Patient ID</th>
      <th>Medhisid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @patienthistories.each do |patienthistory| %>
      <tr>
        <td><%= patienthistory.patientID %></td>
        <td><%= patienthistory.medHisID %></td>
        <td><%= link_to 'Show', patienthistory %></td>
        <td><%= link_to 'Edit', edit_patienthistory_path(patienthistory) %></td>
        <td><%= link_to 'Destroy', patienthistory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Patienthistory', new_patienthistory_path %>
<%= link_to 'Back', dashboard_index_path %>