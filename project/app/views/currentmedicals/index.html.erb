<p id="notice"><%= notice %></p>

<h1>Currentmedicals</h1>

<table>
  <thead>
    <tr>
      <th>currMedID</th>
      <th>Datecreated</th>
      <th>Dateupdated</th>
      <th>Medications</th>
      <th>Allergies</th>
      <th>Symptoms</th>
      <th>Bloodpressure</th>
      <th>Height</th>
      <th>Weight</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @currentmedicals.each do |currentmedical| %>
      <tr>
        <td><%= currentmedical.currMedID %></td>
        <td><%= currentmedical.dateCreated %></td>
        <td><%= currentmedical.dateUpdated %></td>
        <td><%= currentmedical.medications %></td>
        <td><%= currentmedical.allergies %></td>
        <td><%= currentmedical.symptoms %></td>
        <td><%= currentmedical.bloodPressure %></td>
        <td><%= currentmedical.height %></td>
        <td><%= currentmedical.weight %></td>
        <td><%= link_to 'Show', currentmedical %></td>
        <td><%= link_to 'Edit', edit_currentmedical_path(currentmedical) %></td>
        <td><%= link_to 'Destroy', currentmedical, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Currentmedical', new_currentmedical_path %>
<%= link_to 'Back', dashboard_index_path %>